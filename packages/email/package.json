{
  "name": "@dub/email",
  "version": "0.0.0",
  "private": true,
  "main": "./src/index.ts",
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "dev": "email dev --dir ./src/templates -p 3333"
  },
  "dependencies": {
    "@dub/utils": "workspace:*",
    "@react-email/components": "^1.0.2",
    "lucide-react": "^0.562.0",
    "nodemailer": "^7.0.11",
    "react-email": "^5.1.0",
    "resend": "^6.6.0"
  },
  "devDependencies": {
    "@types/nodemailer": "~7.0.4",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    },
    "./templates/*": {
      "import": "./src/templates/*.tsx",
      "require": "./src/templates/*.tsx"
    },
    "./resend": {
      "import": "./src/resend/index.ts",
      "require": "./src/resend/index.ts"
    },
    "./resend/*": {
      "import": "./src/resend/*.ts",
      "require": "./src/resend/*.ts"
    },
    "./send-via-nodemailer": {
      "import": "./src/send-via-nodemailer.ts",
      "require": "./src/send-via-nodemailer.ts"
    }
  }
}
