{
  "name": "@dub/utils",
  "description": "Utility functions and constants for Dub.co",
  "version": "0.1.25",
  "sideEffects": false,
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.mjs"
    }
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint src/",
    "dev": "tsup --watch",
    "check-types": "tsc --noEmit"
  },
  "peerDependencies": {
    "next": "14.2.0-canary.47",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@types/ms": "^2.1.0",
    "@types/node": "25.0.3",
    "@types/punycode": "^2.1.4",
    "@types/react": "^19.2.7",
    "next": "16.0.10",
    "react": "^19.2.3",
    "tsconfig": "workspace:*",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@sindresorhus/slugify": "^3.0.0",
    "chrono-node": "^2.9.0",
    "clsx": "^2.1.1",
    "ms": "^2.1.3",
    "nanoid": "^5.0.1",
    "punycode": "^2.3.1",
    "tailwind-merge": "^3.4.0"
  },
  "author": "Steven Tey <stevensteel97@gmail.com>",
  "homepage": "https://dub.co",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dubinc/dub.git"
  },
  "bugs": {
    "url": "https://github.com/dubinc/dub/issues"
  },
  "keywords": [
    "dub",
    "dub.co",
    "utils",
    "utility",
    "functions"
  ],
  "publishConfig": {
    "access": "public"
  }
}
