{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env"],
  "globalPassThroughEnv": [
    "AUTH_BEARER_TOKEN",
    "CALENDLY_CLIENT_ID",
    "CALENDLY_CLIENT_SECRET",
    "CALENDLY_INTEGRATION_ID",
    "CALENDLY_WEBHOOK_SIGNING_KEY",
    "DATABASE_URL",
    "EDGE_CONFIG",
    "EDGE_CONFIG_ID",
    "GOOGLE_CLIENT_ID",
    "GOOGLE_CLIENT_SECRET",
    "NEXTAUTH_SECRET",
    "NEXTAUTH_URL",
    "PIMMS_API_KEY",
    "PIMMS_PROGRAM_ID",
    "PROJECT_ID_VERCEL",
    "QSTASH_CURRENT_SIGNING_KEY",
    "QSTASH_NEXT_SIGNING_KEY",
    "QSTASH_TOKEN",
    "RESEND_API_KEY",
    "STORAGE_ACCESS_KEY_ID",
    "STORAGE_BASE_URL",
    "STORAGE_ENDPOINT",
    "STORAGE_SECRET_ACCESS_KEY",
    "STRIPE_SECRET_KEY",
    "STRIPE_WEBHOOK_SECRET",
    "TEAM_ID_VERCEL",
    "TINYBIRD_API_KEY",
    "TINYBIRD_API_URL",
    "UNSPLASH_ACCESS_KEY",
    "UPSTASH_REDIS_REST_TOKEN",
    "UPSTASH_REDIS_REST_URL",
    "ZAPIER_INTEGRATION_ID"
  ],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["!.next/cache/**", ".next/**", "dist/**"],
      "passThroughEnv": [
        "AUTH_BEARER_TOKEN",
        "CALENDLY_CLIENT_ID",
        "CALENDLY_CLIENT_SECRET",
        "CALENDLY_INTEGRATION_ID",
        "CALENDLY_WEBHOOK_SIGNING_KEY",
        "DATABASE_URL",
        "EDGE_CONFIG",
        "EDGE_CONFIG_ID",
        "GOOGLE_CLIENT_ID",
        "GOOGLE_CLIENT_SECRET",
        "NEXTAUTH_SECRET",
        "NEXTAUTH_URL",
        "PIMMS_API_KEY",
        "PIMMS_PROGRAM_ID",
        "PROJECT_ID_VERCEL",
        "QSTASH_CURRENT_SIGNING_KEY",
        "QSTASH_NEXT_SIGNING_KEY",
        "QSTASH_TOKEN",
        "RESEND_API_KEY",
        "STORAGE_ACCESS_KEY_ID",
        "STORAGE_BASE_URL",
        "STORAGE_ENDPOINT",
        "STORAGE_SECRET_ACCESS_KEY",
        "STRIPE_SECRET_KEY",
        "STRIPE_WEBHOOK_SECRET",
        "TEAM_ID_VERCEL",
        "TINYBIRD_API_KEY",
        "TINYBIRD_API_URL",
        "UNSPLASH_ACCESS_KEY",
        "UPSTASH_REDIS_REST_TOKEN",
        "UPSTASH_REDIS_REST_URL",
        "ZAPIER_INTEGRATION_ID"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    },
    "test": {
      "dependsOn": ["^build"]
    }
  }
}
